#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="${webModuleName}">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="ru.sendto.gwt.Html" />
	<inherits name="ru.sendto.gwt.client.Notifications" />
	<inherits name="ru.sendto.rest.UniversalRest" />
	<inherits name="ru.sendto.gwt.client.Shortcut" />
	<inherits name="ru.sendto.gwt.client.Bus" />
	<inherits name="ru.sendto.gwt.YT" />
	<inherits name="ru.sendto.DtoCommon" />
	<inherits name="${package}.Dto" />
	<entry-point class="${package}.webapp.client.${webModuleName}" />
	<source path="client" />
	<set-property name="user.agent" value="safari"/>
</module>